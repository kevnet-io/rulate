{
  "schemas": [
    {
      "name": "wardrobe_v2",
      "version": "2.0.0",
      "description": "Gender-agnostic schema with granular coverage-layer modeling",
      "dimensions": [
        {
          "name": "category",
          "type": "enum",
          "required": true,
          "description": "Type of clothing item (gender-agnostic)",
          "values": [
            "shirt",
            "blouse",
            "top",
            "pants",
            "skirt",
            "dress",
            "shorts",
            "jacket",
            "blazer",
            "coat",
            "sweater",
            "cardigan",
            "vest",
            "hoodie",
            "underwear",
            "boxers",
            "briefs",
            "bra",
            "undershirt",
            "slip",
            "tights",
            "leggings",
            "socks",
            "shoes",
            "boots",
            "heels",
            "flats",
            "sneakers",
            "sandals",
            "hat",
            "cap",
            "beanie",
            "scarf",
            "gloves",
            "belt",
            "tie",
            "bowtie",
            "jewelry",
            "necklace",
            "earrings",
            "bracelet",
            "ring",
            "watch"
          ]
        },
        {
          "name": "coverage_layers",
          "type": "part_layer_list",
          "required": true,
          "description": "Body parts covered and their layering positions",
          "part_vocabulary": [
            "head",
            "face",
            "ears",
            "neck",
            "shoulders",
            "chest",
            "upper_back",
            "lower_back",
            "waist",
            "hips",
            "upper_arm",
            "lower_arm",
            "wrist",
            "hands",
            "fingers",
            "groin",
            "upper_leg",
            "lower_leg",
            "ankle",
            "feet",
            "toes"
          ]
        },
        {
          "name": "formality",
          "type": "integer",
          "required": false,
          "description": "Formality level (1=very casual, 5=very formal)",
          "min": 1,
          "max": 5
        },
        {
          "name": "colors",
          "type": "list",
          "item_type": "string",
          "required": false,
          "description": "Primary colors in the item"
        },
        {
          "name": "season",
          "type": "enum",
          "required": false,
          "description": "Appropriate seasons for wearing",
          "values": [
            "spring",
            "summer",
            "fall",
            "winter",
            "all_season"
          ]
        },
        {
          "name": "style",
          "type": "enum",
          "required": false,
          "description": "General style category",
          "values": [
            "casual",
            "business",
            "athletic",
            "formal",
            "streetwear",
            "bohemian",
            "minimalist"
          ]
        }
      ]
    }
  ],
  "rulesets": [
    {
      "name": "wardrobe_rules_v2",
      "version": "2.0.0",
      "description": "Physical compatibility rules (can items be worn together?)",
      "schema_ref": "wardrobe_v2",
      "rules": [
        {
          "name": "coverage_layer_conflict",
          "type": "exclusion",
          "description": "Items cannot have conflicting coverage-layer relationships",
          "enabled": true,
          "condition": {
            "part_layer_conflict": {
              "field": "coverage_layers"
            }
          }
        },
        {
          "name": "same_category_exclusion",
          "type": "exclusion",
          "description": "Cannot wear two items of the same category (with exceptions for jewelry/accessories)",
          "enabled": true,
          "condition": {
            "equals": {
              "field": "category"
            }
          }
        }
      ]
    }
  ],
  "cluster_rulesets": [
    {
      "name": "wardrobe_cluster_rules_v2",
      "version": "2.0.0",
      "description": "Outfit-level aesthetic compatibility rules (moved from pairwise rules)",
      "schema_ref": "wardrobe_v2",
      "pairwise_ruleset_ref": "wardrobe_rules_v2",
      "rules": [
        {
          "name": "minimum_outfit_size",
          "type": "requirement",
          "enabled": true,
          "description": "An outfit must have at least 2 items",
          "condition": {
            "min_cluster_size": 2
          }
        },
        {
          "name": "formality_consistency",
          "type": "requirement",
          "enabled": true,
          "description": "All items must be within 1 formality level of each other",
          "condition": {
            "formality_range": {
              "max_diff": 1
            }
          }
        },
        {
          "name": "season_consistency",
          "type": "requirement",
          "enabled": true,
          "description": "All items should be appropriate for the same season or be all-season",
          "condition": {
            "or": [
              {
                "unique_values": {
                  "field": "season",
                  "max": 1
                }
              },
              {
                "has_item_with": {
                  "field": "season",
                  "value": "all_season"
                }
              }
            ]
          }
        },
        {
          "name": "max_outfit_size",
          "type": "exclusion",
          "enabled": true,
          "description": "An outfit can't have more than 10 items (too complex)",
          "condition": {
            "not": {
              "max_cluster_size": 10
            }
          }
        }
      ]
    }
  ],
