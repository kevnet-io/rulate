{
  "name": "rulate-web",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": ">=22.0.0 <23.0.0",
    "npm": ">=11.0.0"
  },
  "scripts": {
    "sync": "svelte-kit sync",
    "postinstall": "npm run sync",
    "dev": "vite dev",
    "prebuild": "npm run sync",
    "build": "vite build",
    "preview": "vite preview",
    "typecheck": "npm run sync && svelte-check --tsconfig ./tsconfig.json",
    "typecheck:watch": "npm run sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "check": "npm run format:check && npm run lint && npm run typecheck && npm test && npm run build",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prelint": "npm run sync",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:coverage:ui": "vitest --ui --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@sveltejs/adapter-auto": "^3.0.0",
    "@sveltejs/adapter-static": "^3.0.10",
    "@sveltejs/kit": "^2.49.2",
    "@sveltejs/vite-plugin-svelte": "^4.0.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/svelte": "^5.2.10",
    "@types/eslint": "^9.6.0",
    "@vitest/coverage-v8": "^4.0.13",
    "@vitest/ui": "^4.0.13",
    "autoprefixer": "^10.4.16",
    "eslint": "^9.39.2",
    "eslint-plugin-svelte": "^2.36.0",
    "globals": "^16.5.0",
    "happy-dom": "^20.0.10",
    "jsdom": "^27.2.0",
    "postcss": "^8.4.32",
    "prettier": "^3.0.0",
    "svelte": "^5.46.0",
    "svelte-check": "^4.0.0",
    "tailwindcss": "^3.3.6",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.0.0",
    "vite": "^5.0.3",
    "vitest": "^4.0.13"
  },
  "type": "module",
  "dependencies": {
    "clsx": "^2.1.0",
    "tailwind-merge": "^2.2.0",
    "tailwind-variants": "^0.2.0"
  },
  "overrides": {
    "cookie": "^0.7.0",
    "esbuild": "^0.25.0"
  }
}
